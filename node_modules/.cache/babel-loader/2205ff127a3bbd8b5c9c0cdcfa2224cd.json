{"ast":null,"code":"import { ref, watch } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      nomor: \"1\",\n      judul: \"\",\n      arti: \"\",\n      listSurah: ref([]),\n      namaSurah: \"\"\n    };\n  },\n\n  watch: {\n    nomor() {\n      this.getSurah();\n    }\n\n  },\n\n  mounted() {\n    this.getSurah();\n    this.getlistSurah();\n  },\n\n  methods: {\n    getSurah() {\n      axios.get(`https://api.quran.com/api/v4/chapters/${this.nomor}?language=id`).then(response => {\n        this.judul = response.data.chapter;\n        this.arti = this.judul.translated_name;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    getlistSurah() {\n      axios.get(`https://api.quran.com/api/v4/chapters?language=en`).then(response => {\n        this.listSurah = response.data.chapters;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AACA,SAASA,GAAT,EAAcC,KAAd,QAA2B,KAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,GADF;MAELC,KAAK,EAAE,EAFF;MAGLC,IAAI,EAAE,EAHD;MAILC,SAAS,EAACP,GAAG,CAAC,EAAD,CAJR;MAKLQ,SAAS,EAAC;IALL,CAAP;EAOD,CATY;;EAWbP,KAAK,EAAE;IACLG,KAAK,GAAG;MACN,KAAKK,QAAL;IACF;;EAHK,CAXM;;EAiBbC,OAAO,GAAG;IACR,KAAKD,QAAL;IACA,KAAKE,YAAL;EACD,CApBY;;EAsBbC,OAAO,EAAE;IACPH,QAAQ,GAAG;MACTP,KAAI,CACDW,GADH,CACQ,yCAAwC,KAAKT,KAAM,cAD3D,EAEGU,IAFH,CAEQC,QAAO,IAAK;QAChB,KAAKV,KAAL,GAAaU,QAAQ,CAACZ,IAAT,CAAca,OAA3B;QACA,KAAKV,IAAL,GAAY,KAAKD,KAAL,CAAWY,eAAvB;MACD,CALH,EAMGC,KANH,CAMUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CARH;IASD,CAXM;;IAYPR,YAAY,GAAE;MACZT,KAAI,CACHW,GADD,CACM,mDADN,EAECC,IAFD,CAEMC,QAAO,IAAK;QAChB,KAAKR,SAAL,GAAiBQ,QAAQ,CAACZ,IAAT,CAAcmB,QAA/B;MACD,CAJD,EAKCJ,KALD,CAKOC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAPD;IAQF;;EArBO;AAtBI,CAAf","names":["ref","watch","axios","data","nomor","judul","arti","listSurah","namaSurah","getSurah","mounted","getlistSurah","me
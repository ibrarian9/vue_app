{"ast":null,"code":"import { ref, watch } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      nomor: \"1\",\n      judul: \"\",\n      arti: \"\",\n      listSurah: ref([]),\n      namaSurah: \"\",\n      search: \"\"\n    };\n  },\n\n  watch: {\n    nomor() {\n      this.getSurah();\n    }\n\n  },\n\n  mounted() {\n    this.getSurah();\n    this.getlistSurah();\n  },\n\n  methods: {\n    getSurah() {\n      axios.get(`https://api.quran.com/api/v4/chapters/${this.nomor}?language=id`).then(response => {\n        this.judul = response.data.chapter;\n        this.arti = this.judul.translated_name;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    getlistSurah() {\n      axios.get(`https://api.quran.com/api/v4/chapters?language=id`).then(response => {\n        this.listSurah = response.data.chapters;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  },\n  computed: {\n    filterSurah: function () {\n      return this.listSurah.filter(listSurah => {\n        return listSurah.name_simple.toLowerCase().split(\"-\").join(\" \").match(this.search);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA0CA,SAASA,GAAT,EAAcC,KAAd,QAA2B,KAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,GADF;MAELC,KAAK,EAAE,EAFF;MAGLC,IAAI,EAAE,EAHD;MAILC,SAAS,EAAEP,GAAG,CAAC,EAAD,CAJT;MAKLQ,SAAS,EAAE,EALN;MAMLC,MAAM,EAAE;IANH,CAAP;EAQD,CAVY;;EAYbR,KAAK,EAAE;IACLG,KAAK,GAAG;MACN,KAAKM,QAAL;IACD;;EAHI,CAZM;;EAkBbC,OAAO,GAAG;IACR,KAAKD,QAAL;IACA,KAAKE,YAAL;EACD,CArBY;;EAuBbC,OAAO,EAAE;IACPH,QAAQ,GAAG;MACTR,KAAI,CACDY,GADH,CACQ,yCAAwC,KAAKV,KAAM,cAD3D,EAEGW,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKX,KAAL,GAAaW,QAAQ,CAACb,IAAT,CAAcc,OAA3B;QACA,KAAKX,IAAL,GAAY,KAAKD,KAAL,CAAWa,eAAvB;MACD,CALH,EAMGC,KANH,CAMUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CARH;IASD,CAXM;;IAYPR,YAAY,GAAG;MACbV,KAAI,CACDY,GADH,CACQ,mDADR,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKT,SAAL,GAAiBS,QAAQ,CAACb,IAAT,CAAcoB,QAA/B;MACD,CAJH,EAKGJ,KALH,CAKUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAPH;IAQD;;EArBM,CAvBI;EA8CbI,QAAQ,EAAE;IACRC,WAAW,EAAE,YAAY;MACvB,OAAO,KAAKlB,SAAL,CAAemB,MAAf,CAAuBnB,SAAD,IAAe;QAC1C,OAAOA,SAAS,CAACoB,WAAV,CAAsBC,WAAtB,GAAoCC,KAApC,CAA0C,GAA1C,EAA+CC,IAA/C,CAAoD,GAApD,EAAyDC,KAAzD,CAA+D,KAAKtB,MAApE,CAAP;MACD,CAFM,CAAP;IAGD;EALO;AA9CG,CAAf","names":["ref","watch","axios","data","nomor","judul","arti","listSurah","namaSurah","search","getSurah","mounted","getlistSurah","methods","get","then","response","chapter","translated_name","catch","error","console","log","chapters","computed","filterSurah","filter","name_simple","toLowerCase","split","join","match"],"sourceRoot":"","sources":["C:\\Users\\user\\Pictures\\pweb\\quranwithvue-master\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n\n  <!-- JumboTron -->\n  <div class=\"jumbotron jumbotron-fluid mt-5\">\n    <div class=\"container text-center\">\n      <br>\n      <h1 class=\"mt-5 display-4 fw-bolder\">Alquran Flexibel</h1>\n      <p class=\"lead\">Bisa diakses kapan saja</p>\n    </div>\n  </div>\n  \n  <!-- container searchbox -->\n  <div class=\"container\">\n    <div class=\"row align-item-center justify-content-center\">\n      <form class=\"col-xl-12 col-md-12 col-lg-12 col-sm-12 col-12 p-1 mt-4\">\n        <input class=\"form-control form-control-lg mb-1 font-italic\" type=\"text\" v-model=\"search\" placeholder=\"cari surah yang kamu inginkan...\" />\n      </form>\n    </div>\n  </div>\n\n  <!-- Card Surah -->\n  <div class=\"content-surah mx-5\">\n    <div class=\"row\">\n      <div class=\"col-xl-3 col-sm-6 col-12 p-1\" v-for=\"(chapter, index) in filterSurah\" :key=\"index\">\n        <div class=\"card mx-3 my-3\">\n          <div class=\"card-content\">\n            <div class=\"card-body\">\n              <div class=\"media d-flex\">\n                <div  class=\"media-body m-auto text-center\">\n                  <h5 class=\"card-title\">{{ chapter.name_simple }}</h5>\n                  <p class=\"card-text\">{{ chapter.translated_name.name }}</p>\n                  <router-link :to=\"{ name: 'baca', params: { id: chapter.id } }\" id=\"warna\" class=\"btn btn\">View </router-link>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { ref, watch } from \"vue\";\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      nomor: \"1\",\n      judul: \"\",\n      arti: \"\",\n      listSurah: ref([]),\n      namaSurah: \"\",\n      search: \"\",\n    };\n  },\n\n  watch: {\n    nomor() {\n      this.getSurah();\n    },\n  },\n\n  mounted() {\n    this.getSurah();\n    this.getlistSurah();\n  },\n\n  methods: {\n    getSurah() {\n      axios\n        .get(`https://api.quran.com/api/v4/chapters/${this.nomor}?language=id`)\n        .then((response) => {\n          this.judul = response.data.chapter;\n          this.arti = this.judul.translated_name;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    getlistSurah() {\n      axios\n        .get(`https://api.quran.com/api/v4/chapters?language=id`)\n        .then((response) => {\n          this.listSurah = response.data.chapters;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  computed: {\n    filterSurah: function () {\n      return this.listSurah.filter((listSurah) => {\n        return listSurah.name_simple.toLowerCase().split(\"-\").join(\" \").match(this.search);\n      });\n    },\n  },\n};\n</script>\n<style scoped>\n.card {\n  border-radius: 20pt;\n}\n\n#warna{\n  background-color: #FFDE59;\n}\n</style>\n>\n"]},"metadata":{},"sourceType":"module"}
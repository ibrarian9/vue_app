{"ast":null,"code":"import { ref } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      nomor: \"1\",\n      judul: \"\",\n      arti: \"\",\n      listSurah: ref([]),\n      namaSurah: \"\"\n    };\n  },\n\n  watch: {\n    nomor() {\n      this.getSurah();\n    }\n\n  },\n\n  mounted() {\n    this.getSurah();\n    this.getlistSurah();\n  },\n\n  methods: {\n    getSurah() {\n      axios.get(`https://api.quran.com/api/v4/chapters/${this.nomor}?language=id`).then(response => {\n        this.judul = response.data.chapter;\n        this.arti = this.judul.translated_name;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    getlistSurah() {\n      axios.get(`https://api.quran.com/api/v4/chapters?language=id`).then(response => {\n        this.listSurah = response.data.chapters;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BA,SAASA,GAAT,QAAoB,KAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAc;EACZC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,GADF;MAELC,KAAK,EAAE,EAFF;MAGLC,IAAI,EAAE,EAHD;MAILC,SAAS,EAAEN,GAAG,CAAC,EAAD,CAJT;MAKLO,SAAS,EAAE;IALN,CAAP;EAOD,CATW;;EAWZC,KAAK,EAAE;IACLL,KAAK,GAAG;MACN,KAAKM,QAAL;IACD;;EAHI,CAXK;;EAiBZC,OAAO,GAAG;IACR,KAAKD,QAAL;IACA,KAAKE,YAAL;EACD,CApBW;;EAsBZC,OAAO,EAAE;IACPH,QAAQ,GAAG;MACTR,KAAI,CACDY,GADH,CACQ,yCAAwC,KAAKV,KAAM,cAD3D,EAEGW,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKX,KAAL,GAAaW,QAAQ,CAACb,IAAT,CAAcc,OAA3B;QACA,KAAKX,IAAL,GAAY,KAAKD,KAAL,CAAWa,eAAvB;MACD,CALH,EAMGC,KANH,CAMUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CARH;IASD,CAXM;;IAYPR,YAAY,GAAG;MACbV,KAAI,CACDY,GADH,CACQ,mDADR,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKT,SAAL,GAAiBS,QAAQ,CAACb,IAAT,CAAcoB,QAA/B;MACD,CAJH,EAKGJ,KALH,CAKUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAPH;IAQD;;EArBM;AAtBG,CAAd","names":["ref","axios","data","nomor","judul","arti","listSurah","namaSurah","watch","getSurah","mounted","getlistSurah","methods","get","th